{
  "name": "@userdocks/web-client-sdk",
  "version": "0.16.0",
  "description": "The JavaScript web client SDK for userdocks. Securly store, access, and refresh access tokens in your browser application.",
  "source": "./src/index.ts",
  "main": "./dist/userdocks.js",
  "module": "./dist/userdocks.modern.js",
  "unpkg": "./dist/userdocks.umd.js",
